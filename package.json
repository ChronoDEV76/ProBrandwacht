{
  "name": "probrandwacht",
  "private": true,
  "version": "1.1.1",
  "type": "module",
  "scripts": {
    "build": "next build",
    "analyze": "ANALYZE=true next build",
    "dev": "next dev",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "e2e": "playwright test",
    "qa:cta-audit": "node scripts/qa/cta-audit.mjs",
    "qa:copy-audit": "node scripts/qa/copy-audit-app.mjs",
    "qa:blog-risk": "node scripts/qa/blog-risk-scan.mjs",
    "qa:legal-sanity": "node scripts/qa/legal-sanity-check.mjs --root . --config scripts/qa/legal-sanity.config.json",
    "freeze:guard": "node scripts/freeze/freeze-guard.mjs",
    "check:copy": "ts-node scripts/content/checkCopy.ts",
    "blog:link-check": "node scripts/content/blog-link-check.mjs",
    "tone-check": "node scripts/tone/toneCheck.mjs --local=1 --root content",
    "sanity": "node scripts/qa/sanityCheck.mjs --roots=app,content,components --min-sent-len=80 --max-sent-len=220 --max-par-len=800",
    "content-check": "node --import tsx scripts/content/launch-copy-check.ts && npm run blog:link-check && npm run blog:vr-guard && npm run sanity && npm run tone-check && npm run frictie:guard && npm run freeze:guard",
    "sanity:probrandwacht": "node scripts/qa/probrandwacht-sanity-guard.mjs --root . --config scripts/qa/probrandwacht-sanity-guard.config.json",
    "blog-guard": "node scripts/blog/blog-guard-oom.mjs --root . --config scripts/blog/blog-guard.config.json",
    "blog:guard:strict": "node scripts/blog/blog-guard-oom.mjs",
    "blog:vr-guard": "node scripts/blog/vr-blog-guard.mjs --local=1",
    "seo:check": "node scripts/seo/seo-check.mjs --local=1",
    "seo:check:tsx": "tsx seo-checker.ts",
    "seo:audit": "node scripts/seo/digitalSeoAudit.mjs --site=https://www.probrandwacht.nl",
    "seo:audit:legacy": "node scripts/seo/seoAudit.mjs --start=https://www.probrandwacht.nl --max-pages=50 --output=seo_report.json --md=seo_report.md",
    "seo:dashboard": "node scripts/seo/seo-dashboard.mjs",
    "seo:upgrade": "node scripts/seo/seo-upgrade.mjs",
    "seo:upgrade:apply": "node scripts/seo/seo-upgrade.mjs --apply",
    "seo:upgrade:sitemap": "node scripts/seo/seo-upgrade.mjs --apply --sitemap",
    "quality-check": "npm run seo:check && npm run content-check",
    "frictie:guard": "node scripts/brandwacht-frictie-guard.mjs",
    "launch:check": "node scripts/ops/launch-check.mjs",
    "launch:quick": "node scripts/ops/launch-check.mjs --no-build",
    "diagnose": "node scripts/ops/diagnose.js",
    "fix:metadata": "node scripts/utils/fix-next-metadata.mjs",
    "images:optimize": "node scripts/utils/optimize-images.mjs",
    "find-components": "node scripts/utils/find-components.mjs",
    "add-anchor": "node scripts/utils/add-anchor.mjs",
    "repo:map": "node scripts/utils/repo-map.mjs --format=all",
    "repo:map:small": "node scripts/utils/repo-map.mjs --max-depth=3",
    "repo:map:sizes": "node scripts/utils/repo-map.mjs --format=all --sizes --ext-stats",
    "strip-calculator": "node scripts/utils/strip-calculator.mjs",
    "strip-calculator:dry": "node scripts/utils/strip-calculator.mjs --auto-detect --dry",
    "perf:mobile": "npx @lhci/cli autorun --collect.settings.formFactor=mobile --collect.settings.screenEmulation.mobile=true --collect.startServerCommand=\"npm run start\" --collect.startServerReadyPattern=\"Started server on\" --collect.url=http://localhost:3000 --upload.target=temporary-public-storage",
    "perf:page": "npx @lhci/cli collect --url=http://localhost:3000 --settings.preset=mobile && npx @lhci/cli assert",
    "sanity:aider": "bash scripts/ops/aider-sanity.sh",
    "prepare": "husky"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@next/mdx": "^14.2.33",
    "@slack/bolt": "^4.6.0",
    "@supabase/supabase-js": "^2.77.0",
    "@vercel/kv": "^3.0.0",
    "@vercel/postgres": "^0.10.0",
    "chalk": "^4.1.2",
    "cheerio": "^1.1.2",
    "clsx": "^2.1.1",
    "coffee-script": "^1.12.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "express": "^5.1.0",
    "fs-extra": "^11.3.2",
    "glob": "^13.0.0",
    "jiti": "^1.21.0",
    "next": "^14.2.35",
    "next-mdx-remote": "^5.0.0",
    "next-seo": "^6.8.0",
    "openai": "^6.1.0",
    "pdf-lib": "^1.17.1",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.63.0",
    "react-markdown": "^10.1.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.1",
    "sharp": "^0.34.4",
    "toml": "^3.0.0",
    "undici": "^7.16.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.55.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.19.24",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vitest/utils": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.35",
    "eslint-config-prettier": "^9.1.2",
    "fast-glob": "^3.3.3",
    "gray-matter": "^4.0.3",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "next-sitemap": "^4.2.3",
    "node-fetch": "^3.3.2",
    "playwright": "^1.55.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^3.4.18",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  },
  "overrides": {
    "@next/eslint-plugin-next": {
      "glob": "^10.5.0"
    }
  },
  "packageManager": "npm@10.9.3",
  "peerDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "18",
    "@types/react-dom": "18",
    "typescript": "5.6.3"
  }
}
